{
  "title": "ObjectPort",
  "version": "0.9.1-rc1",
  "authors": [ "Dmytro Mukalov" ],
  "copyright": "Copyright ©  2016 Dmytro Mukalov and Contributors",
  "description": "Ultra fast binary serializer which allows to serialize object graphs with arbitrary complexity, supports anonymous types serialization, supports .NET Core.",
  "packOptions": {
    "owners": [ "Dmytro Mukalov" ],
    "licenseUrl": "https://github.com/mukalov/ObjectPort/blob/master/LICENSE",
    "projectUrl": "https://github.com/mukalov/ObjectPort",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/mukalov/ObjectPort"
    },
    "tags": [ "serialization", "serializer", "binary", "netcore", "core" ]
  },
  "language": "en-US",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "allowUnsafe": true
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "xmlDoc": true,
        "keyFile": "ObjectPort.snk",
        "strongName": true,
        "allowUnsafe": true
      }
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },

  "frameworks": {
    "net40": {
      "frameworkAssemblies": {
        "System": "",
        "System.Core": ""
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System": "",
        "System.Core": ""
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System": "",
        "System.Core": ""
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NETCORE" ]
      },
      "imports": "dnxcore50"
    }
  }
}
